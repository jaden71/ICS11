import java.text.DecimalFormat;
public class Lol {
  
  
  public static void main(String[] args) { 
    int[] randList = new int[10];
    int count = 0;
    for (int i = 0; i < 9; i++)
    {
      randList[i] = (int) Math.floor(Math.random() * 10) + 1;
      if (randList[i] % 2 == 0)
        count++;
    }
    for (int i = 9; i > 1; i--)
    {
      System.out.print(randList[i] + ", ");
      
    }
    System.out.print(randList[0] + "\n");
    System.out.println(count);
  }
    
}
